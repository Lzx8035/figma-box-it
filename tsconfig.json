{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es6",
    "declaration": false,
    "noImplicitAny": false,
    "allowJs": false,
    "sourceMap": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "lib": [ "dom", "es2016" ]
  },
  "exclude": [ "node_modules" ]
}